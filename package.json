{
  "name": "graphql-typeorm",
  "version": "0.1.0",
  "description": "GraphQL for MySQL & Postgres via TypeORM",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucianlature/graphql-typeorm.git"
  },
  "author": "Lucian Lature",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucianlature/graphql-typeorm/issues"
  },
  "homepage": "https://github.com/lucianlature/graphql-typeorm#readme",
  "dependencies": {
    "graphql": "^0.8.2",
    "pg": "^6.1.2",
    "reflect-metadata": "^0.1.9",
    "sqlite3": "^3.1.8",
    "typeorm": "^0.0.6"
  },
  "peerDependencies": {
    "graphql": "0.8.2"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "babel-cli": "~6.18.0",
    "babel-core": "~6.21.0",
    "babel-eslint": "~7.1.1",
    "babel-jest": "~18.0.0",
    "babel-plugin-transform-async-to-generator": "~6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.18.0",
    "babel-plugin-transform-flow-strip-types": "~6.21.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-stage-3": "~6.17.0",
    "eslint": "~3.12.2",
    "eslint-config-airbnb-base": "~11.0.0",
    "eslint-plugin-flowtype": "~2.29.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-no-async-without-await": "~1.2.0",
    "express": "4.14.0",
    "express-graphql": "0.6.1",
    "flow-bin": "~0.37.0",
    "graphql-relay": "0.4.4",
    "jest": "~18.0.0",
    "lerna": "2.0.0-beta.32"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
