{
  "name": "graphql-typeorm",
  "version": "0.1.0",
  "description": "GraphQL for MySQL & Postgres via TypeORM",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint src/*.ts",
    "tslint": "tslint -c tslint.json 'src/*.ts'",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "mocha __tests__/resolver.test.js --require source-map-support/register || true",
    "test:watch": "jest --watch",
    "build": "tsc __tests__/resolver.test.ts || true",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucianlature/graphql-typeorm.git"
  },
  "author": "Lucian Lature",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucianlature/graphql-typeorm/issues"
  },
  "homepage": "https://github.com/lucianlature/graphql-typeorm#readme",
  "dependencies": {
    "graphql": "^0.8.2",
    "pg": "6.1.2",
    "reflect-metadata": "0.1.9",
    "source-map-support": "0.4.10",
    "typeorm": "0.0.6"
  },
  "peerDependencies": {
    "graphql": "0.8.2"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "babel-eslint": "7.1.1",
    "eslint": "3.13.1",
    "eslint-config-cleanjs": "4.0.0",
    "eslint-plugin-better": "0.1.5",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-no-async-without-await": "1.2.0",
    "eslint-plugin-typescript": "0.1.0",
    "jest": "18.1.0",
    "mocha-typescript": "1.0.15",
    "ramda": "0.23.0",
    "ramda-ts": "0.1.2",
    "rimraf": "2.5.4",
    "typescript": "2.1.5",
    "typescript-eslint-parser": "1.0.2",
    "typings": "2.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
