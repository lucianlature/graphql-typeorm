{
  "name": "graphql-typeorm",
  "version": "0.1.0",
  "description": "GraphQL for MySQL & Postgres via TypeORM",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucianlature/graphql-typeorm.git"
  },
  "author": "Lucian Lature",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucianlature/graphql-typeorm/issues"
  },
  "homepage": "https://github.com/lucianlature/graphql-typeorm#readme",
  "dependencies": {
    "graphql": "0.8.2",
    "pg": "6.1.2",
    "reflect-metadata": "0.1.9",
    "source-map-support": "0.4.10",
    "typeorm": "0.0.6"
  },
  "peerDependencies": {
    "graphql": "0.8.2"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "jest": "18.1.0",
    "rimraf": "2.5.4",
    "typescript": "2.2.0-dev.20170118",
    "typings": "2.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
